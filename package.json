{
	"name": "wpgraphql-repl",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm prepare && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "pnpm smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme -i ./node_modules"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.29.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"@sentry/sveltekit": "^7.52.1",
		"@smui-extra/accordion": "7.0.0-beta.8",
		"@smui/card": "7.0.0-beta.8",
		"@smui/icon-button": "7.0.0-beta.8",
		"@smui/list": "7.0.0-beta.8",
		"@smui/paper": "7.0.0-beta.8",
		"@smui/select": "7.0.0-beta.8",
		"@smui/switch": "7.0.0-beta.8",
		"@smui/textfield": "7.0.0-beta.8",
		"@smui/tooltip": "7.0.0-beta.8",
		"@wp-playground/blueprints": "^0.1.46",
		"@wp-playground/client": "^0.1.46",
		"http-status-codes": "^2.2.0",
		"just-extend": "^6.2.0",
		"normalize.css": "^8.0.1",
		"smui-theme": "7.0.0-beta.8"
	}
}
