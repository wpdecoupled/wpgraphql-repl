{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"prepare",
				"^build"
			],
			"outputs": [
				".svelte-kit/**",
				".vercel/**",
				"dist/**"
			]
		},
		"prepare": {
			"outputs": [
				"static/smui.css"
			]
		},
		"lint": {},
		"format": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
