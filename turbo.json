{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"wpgraphql-repl#build": {
			"dependsOn": [
				"prepare",
				"^build"
			],
			"env": [
				"PUBLIC_SUPABASE_URL",
				"PUBLIC_SUPABASE_ANON_KEY"
			],
			"outputs": [
				".svelte-kit/**",
				".vercel/**"
			]
		},
		"wpgraphql-playground#build": {
			"outputs": [
				"dist"
			]
		},
		"prepare": {
			"outputs": [
				"static/smui.css"
			]
		},
		"lint": {},
		"format": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
